# Auto detect text files and perform LF normalization
* text=auto

# Force specific files to use LF
*.md text eol=lf
*.py text eol=lf
*.yml text eol=lf
*.json text eol=lf
*.js text eol=lf
*.jsx text eol=lf
*.ts text eol=lf
*.tsx text eol=lf
*.css text eol=lf
*.scss text eol=lf
*.html text eol=lf
Dockerfile text eol=lf
.dockerignore text eol=lf